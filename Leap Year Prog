import math
import matplotlib.pyplot as plt 
import numpy as np 
import array as arr 
import pandas as pd 


#Leap year program
year = int(input('Enter the year' '\n'))

if year % 400 == 0:
	print('leap year')
elif year % 4 == 0:
	print('leap year')
elif year % 100 == 0:
	print('not a leap year')

else:
	print('not a leap year')
