def add(a,b):
	return a + b 

def sub(a,b):
	return a - b 

def mul(a,b):
	return a * b 

def divide(a,b):
	return a/b 

def si(p,r,t) :  #Simple INterest
	return (p*r*t) / 100

def ci(P,R,T) :
	return P * pow((1 + R/100),T)

def sqr(num):
	return num**2

def st(num):
	return num** 0.5

print(add(10,15))
print(mul(10,13))
print(si(10000,10,5))
